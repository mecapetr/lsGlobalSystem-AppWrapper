{
	"webViewUrl": "https://my.astral.university",
	"expo": {
		"name": "Astral University",
		"slug": "AppWrapper",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/icon_au.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"scheme": "astralportal",
		"splash": {
			"image": "./assets/splash_icon_au.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"icon": "./assets/icon_au.png",
			"supportsTablet": true,
			"bundleIdentifier": "university.astral.portal",
			"infoPlist": {
				"ITSAppUsesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/splash_icon_au.png",
				"backgroundColor": "#ffffff"
			},
			"edgeToEdgeEnabled": true
		},
		"web": {
			"favicon": "./assets/favicon_au.png"
		},
		"extra": {
			"eas": {
				"projectId": "ca0e2244-231b-4575-8406-38af845416ad"
			}
		},
		"plugins": [
			"expo-secure-store",
			"expo-web-browser",
			[
				"expo-splash-screen",
				{
					"backgroundColor": "#ffffff",
					"image": "./assets/splash_icon_au.png",
					"dark": {
						"image": "./assets/splash_icon_au.png",
						"backgroundColor": "#222222"
					},
					"imageWidth": 200
				}
			]
		]
	}
}